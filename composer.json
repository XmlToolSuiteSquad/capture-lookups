{
    "name": "xmlsquad/capture-lookups",
    "description": "Grabs Google Sheets and stores their data as CSV files",
    "type": "library",
    "license": "Apache-2.0",
    "bin": [
        "bin/capture-lookups"
    ],
    "autoload": {
        "psr-4": {
            "XmlSquad\\CaptureLookups\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "XmlSquad\\CaptureLookups\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.1",
        "symfony/console": "^3.4",
        "symfony/filesystem": "^3.4",
        "symfony/yaml": "^3.4",
        "google/apiclient": "^2.0",
        "xmlsquad/xml-authoring-library": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.1",
        "friendsofphp/php-cs-fixer": "^2.11"
    },
    "extra": {
        "branch-alias": null
    }
}
